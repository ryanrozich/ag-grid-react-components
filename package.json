{
  "name": "ag-grid-date-filter",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/ag-grid-date-filter.umd.js",
  "module": "dist/ag-grid-date-filter.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "demo": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "test": "vitest run && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:browser": "node ./scripts/validate-demo.js",
    "test:thorough": "node ./scripts/thorough-demo-check.js",
    "test:filter-click": "node ./scripts/test-filter-click.js",
    "run-tsx": "node ./scripts/run-tsx.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run test && npm run build",
    "postinstall": "playwright install"
  },
  "peerDependencies": {
    "ag-grid-community": "^33.3.0",
    "ag-grid-react": "^33.3.0",
    "date-fns": "^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "ag-grid-enterprise": {
      "optional": true
    }
  },
  "keywords": [
    "ag-grid",
    "date-filter",
    "react",
    "typescript"
